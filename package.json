    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --open",
    "test": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watchAll"
  },
  "dependencies": {
    "@sentry/browser": "^5.24.2",
    "@sentry/integrations": "^5.24.2",
